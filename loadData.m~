function [ output_args ] = loadData(nrange,interval)
a = prnist(nrange,[1:interval:1000]);
hogdata = [];
data = [];

labels = zeros(1, 250); %zeros(10, 25);

period = 1000/interval;

for i = nrange
    for j = 0:(interval01)
        index = 25 * i + j;
        nist = a(index + 1);
        im = data2im(nist);
        imr = imresize(im, [32, 32]);
        data(index + 1, :) = imr(:);
        hog = HOG(imr);
        hogdata(index+1,:) = hog(:);
        labels(index + 1) = i;
    end
end

end

